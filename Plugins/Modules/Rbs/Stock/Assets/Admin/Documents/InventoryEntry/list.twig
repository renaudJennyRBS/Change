<div class="row">
	<div data-rbs-full-width="">
		<rbs-page-header title="{{ i18nAttr('m.rbs.stock.admin.inventoryentry_list', ['ucf']) }}"></rbs-page-header>
		<rbs-document-filter-panel open-by-default="false"
				model="Rbs_Stock_InventoryEntry" filter="filter" ></rbs-document-filter-panel>

		<div class="add-elements-bar">
			<a href data-ng-href="(= 'Rbs_Stock_InventoryEntry' | rbsURL:'new' =)">{{ i18n('m.rbs.stock.admin.inventoryentry_create', ['ucf']) }}</a>
		</div>

		<rbs-document-list data-dlid="DLRbsStockInventoryEntry"
				model="Rbs_Stock_InventoryEntry" filter-collection="filter">
			<rbs-column name="sku" sort="sku.code" primary="true"><a data-ng-href="(= doc | rbsURL =)">(= doc.sku.label =)</a></rbs-column>
			<rbs-column name="warehouse">
				<span data-ng-if="!doc.warehouse.label">{{ i18n('m.rbs.store.admin.global_warehouse', ['ucf']) }}</span>
				<span data-ng-if="doc.warehouse.label">(= doc.warehouse.label =)</span>
			</rbs-column>
			<rbs-column name="level"></rbs-column>
		</rbs-document-list>
	</div>
</div>