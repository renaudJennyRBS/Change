<div class="row" data-rbs-document-editor-base="" data-model="Rbs_Price_UserGroup">
	<div class="col-md-12">
		<rbs-page-header title="{{ i18nAttr('m.rbs.admin.adminjs.new_resource', ['ucf']) }}"
			sub-title="{{ i18nAttr(modelKey('Rbs_Price_UserGroup'), ['ucf']) }}"></rbs-page-header>
	</div>

	<div data-rbs-aside-column="">
		{% include '@Rbs_Admin/view/asides.twig' %}
	</div>

	<div class="document-form" data-rbs-main-column="">
		<form data-ng-submit="submit()" data-rbs-document-editor-new=""
			data-rbs-document-editor-rbs-user-group-new=""
			data-rbs-document-editor-rbs-price-user-group-new="">
			<fieldset data-rbs-editor-section=""
				data-editor-section-label="{{ i18nAttr('m.rbs.admin.admin.general_properties', ['ucf']) }}">

				<rbs-field-text property="label" required="true"
					label="{{ i18nAttr(propertyKey('Rbs_Price_UserGroup', 'label'), ['ucf']) }}">
				</rbs-field-text>

				<rbs-field-integer property="defaultPriority"
						label="{{ i18nAttr(propertyKey('Rbs_Price_UserGroup', 'defaultPriority'), ['ucf']) }}">
				</rbs-field-integer>

				<rbs-field-integer  property="realm"
					label="{{ i18nAttr(propertyKey('Rbs_Price_UserGroup', 'realm'), ['ucf']) }}">
				</rbs-field-integer >

				<rbs-field-text property="identifier"
					label="{{ i18nAttr(propertyKey('Rbs_Price_UserGroup', 'identifier'), ['ucf']) }}">
				</rbs-field-text>
			</fieldset>

			<fieldset data-rbs-editor-section="systeminfo"
				data-editor-section-label="{{ i18nAttr('m.rbs.admin.adminjs.status', ['ucf']) }}"
				data-rbs-document-system-info-section="">
			</fieldset>

			<rbs-document-editor-button-bar></rbs-document-editor-button-bar>
		</form>
	</div>
</div>