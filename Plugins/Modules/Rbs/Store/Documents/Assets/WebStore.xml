<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.rbs.fr/schema/change-document/4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.rbs.fr/schema/change-document/4.0 http://www.rbschange.fr/schema/4.0/document.xsd" 
	editable="true" localized="true">
	<properties>
		<property name="title" type="String" localized="true" />
		<property name="billingAreas" type="DocumentArray" document-type="Rbs_Price_BillingArea" required="true" />
		<property name="displayPricesWithoutTax" type="Boolean" default-value="false" />
		<property name="displayPricesWithTax" type="Boolean" default-value="true" />
		<property name="pricesValueWithTax" type="Boolean" default-value="false" />
		<property name="favoriteWebsiteId" type="DocumentId" document-type="Rbs_Website_Website" />
		<property name="orderProcess" type="Document" document-type="Rbs_Commerce_Process" />
	</properties>
</document>