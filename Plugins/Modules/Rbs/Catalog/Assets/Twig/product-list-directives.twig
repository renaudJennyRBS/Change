{% block rbsCatalogProductsList %}
<script type="text/ng-template" id="/rbsCatalogProductsList.tpl">
	<ul class="grid-list grid-list-{{ itemsPerLine }} grid-list-bordered grid-list-align-top product-list"
			data-infinite-scroll="addMoreProducts()" data-infinite-scroll-container=""
			data-infinite-scroll-disabled="scrollDisabled()">
		<li data-ng-repeat="productData in productsData">

			<div class="grid-list-cell" data-rbs-catalog-product-item-data="">
				<div class="product-visual">
					<a title="(= viewDetailTitle =)" data-ng-href="(= url =)" href="javascript:">
						<img data-ng-if="visual.listItem" alt="(= visual.alt =)" src="" data-ng-src="(= visual.listItem =)" />
						<div class="text-center text-muted" data-ng-if="!visual.listItem">
							<span class="glyphicon glyphicon-camera" style="font-size: 100px"></span>
						</div>
					</a>
					<div data-rbs-catalog-product-pictograms="" data-pictogram-format="'pictogram'" data-pictograms="pictograms"></div>
				</div>
				<div class="product-label">
					<a title="(= viewDetailTitle =)" data-ng-href="(= url =)" href="javascript:">(= productData.common.title =)</a>
				</div>
				<div class="product-availability">(= productData.stock.thresholdTitle =) </div>
				<div data-rbs-catalog-product-price=""
						data-display-without-tax="(= parameters.displayPricesWithoutTax ? '1' : '0' =)"
						data-display-with-tax="(= parameters.displayPricesWithTax ? '1' : '0' =)"></div>
				<div class="product-add" data-ng-if="productData.stock.sku">
					<div data-ng-if="productData.cart.key" data-rbs-catalog-add-list-item-product-to-cart="" data-modal-id="addToCartConfirmationModal-{{ blockId }}">
					</div>
					<button data-ng-if="!productData.cart.key"   type="button" class="btn btn-primary btn-sm" disabled="disabled">
						{{ i18n('m.rbs.commerce.front.not_available', ['ucf']) }}
					</button>
				</div>
				<div class="product-add" data-ng-if="!productData.stock.sku">
					<a class="btn btn-primary btn-sm"  data-ng-href="(= url =)" href="javascript:"
							title="(= viewDetailTitle =)">
						<i class="glyphicon glyphicon-arrow-right"></i>
						(= viewDetailTitle =)
					</a>
				</div>
			</div>
		</li>
	</ul>

	<div class="text-center well" data-ng-show="loading">
		<p data-ng-hide="message">{{ i18n('t.rbs.base.front.loading', ['ucf', 'etc']) }}</p>
		<img alt="" src="data:image/gif;base64," />
	</div>
</script>
{% endblock rbsCatalogProductsList %}